<h1>GenieACS <%= @genieac.name %></h1>
<TABLE>
<THEAD>
    <TR>
        <TD width="1%">
           Name:
        </TD>
        <TD>
			<%= @genieac.name %>
        </TD>
    </TR>
</THEAD>
    <TR>
        <TD width="1%">
          IP addr:
        </TD>
        <TD>
			<%= @genieac.url %>
        </TD>
    </TR>
    <TR>
        <TD width="1%">
           Password:
        </TD>
        <TD>
			<%= @genieac.passwd %>
        </TD>
    </TR>
    <TR class="foot">
        <TD colspan="2">
        <%= link_to "Edit", edit_genieac_path(@genieac) %>
        <%= link_to "Destroy", genieac_path(@genieac), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %>
        </TD>
    </TR>
</TABLE>

